{
    "name": "DemoTheme",

    "sencha": {
        "namespace": "Ext",
        "type": "theme",
        "extend": "theme-material",

        "requires": [],

        "version": "1.0.0",
        "compatVersion": "1.0.0",
        "format": "1",
        "local": true,

        "framework": "ext",
        "toolkit": "modern",

        "slicer": null,

        "output": "${package.dir}/build",

        "classpath": [
            "${package.dir}/src"
        ],

        "overrides": [
            "${package.dir}/overrides"
        ],

        "sass" : {
            "namespace": "Ext",

            "etc": [
                "${package.dir}/sass/etc/all.scss"
             ],

            "var": [
                "${package.dir}/sass/var/all.scss",
                "${package.dir}/sass/var"
            ],

            "src": [
                "${package.dir}/sass/src"
            ]
        }
    }
}
