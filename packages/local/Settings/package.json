{
    "name": "Settings",

    "sencha": {
        "namespace": "Demo.settings",
        "type": "code",
    
        "requires": [
            "Alerts"
        ],

        "version": "1.0.0",
        "compatVersion": "1.0.0",
        "format": "1",
        "local": true,
    
        "slicer": null,

        "output": "${package.dir}/build",
    
        "classpath": [
            "${package.dir}/src"
        ],
    
        "overrides": [
            "${package.dir}/overrides"
        ],
    
        "sass" : {
            "namespace": "Demo.settings",

            "etc": [
                "${package.dir}/sass/etc/all.scss"
             ],
    
            "var": [
                "${package.dir}/sass/var"
            ],
    
            "src": [
                "${package.dir}/sass/src"
            ]
        }
    }
}
